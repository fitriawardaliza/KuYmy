<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kuymy_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KuYmy_Project">
        <activity android:name="views.PesananBerhasil"></activity>
        <activity android:name="views.ListView" />
        <activity android:name="views.Pesan" />
        <activity android:name="views.DaftarCemilanActivity" />
        <activity android:name="views.DaftarMinumanActivity" />
        <activity android:name="views.DaftarMakananActivity" />
        <activity android:name="views.MenuUtama" />
        <activity android:name="views.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>